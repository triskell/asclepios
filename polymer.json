{
  "entrypoint": "index.html",
  "shell": "src/asclepios-app.html",
  "fragments": [
    "src/asclepios-diary.html",
    "src/asclepios-parameters.html",
    "src/asclepios-about.html",
    "src/asclepios-view404.html",
    "src/asclepios-form-weight.html",
    "src/asclepios-form-meal.html",
    "src/asclepios-form-activity.html",
    "src/asclepios-form-mood.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "name": "asclepios",
      "preset": "es5-bundled",
      "basePath": true
    }
  ]
}
